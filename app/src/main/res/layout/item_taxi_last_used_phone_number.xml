<md.fusionworks.lifehack.widget.SquareFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:view="http://schemas.android.com/apk/res-auto"
    android:layout_width="0dp"
    style="@style/TaxiPhoneNumberStyle"
    android:layout_height="wrap_content"
    android:layout_weight="25"
    android:visibility="gone"
    >
  <md.fusionworks.lifehack.widget.TypefaceTextView
      android:id="@+id/phoneNumberField"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_gravity="center"
      android:gravity="center"
      android:padding="20dp"
      android:singleLine="true"
      android:text=""
      android:textColor="@color/white"
      android:textSize="14sp"
      view:typeface="font/Roboto-Bold.ttf"
      />
</md.fusionworks.lifehack.widget.SquareFrameLayout>